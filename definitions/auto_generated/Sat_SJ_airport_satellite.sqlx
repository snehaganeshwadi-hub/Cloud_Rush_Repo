
config {
  type: "table",
  tags: ["satellite"],
  schema: "Row_Vault",
  partitionBy: "load_dts",
  clusterBy: ["AirportCode_bk"]
}
 
SELECT
  TO_HEX(SHA256(CAST(AirportCode AS STRING))) AS AirportCode_hk,
  AirportCode AS AirportCode_bk,
  AirportName,
  CURRENT_DATE() AS load_dts,
  'SpiceJet_AirPort_Details' AS record_source
FROM
  ${ref("SpiceJet_AirPort_Details")}
